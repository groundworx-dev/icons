{
  "name": "@groundworx/icons",
  "version": "1.0.0",
  "description": "Shared icon components for use across Groundworx blocks and plugins.",
  "author": "Johanne Courtright",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "module",
  "sideEffects": false,
