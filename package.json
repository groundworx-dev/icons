  },
  "homepage": "https://github.com/groundworx-dev/icons#readme",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@wordpress/babel-preset-default": "^8.23.0"
  }
